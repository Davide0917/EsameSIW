<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<display-name>searchouse</display-name>
	<error-page>
    	<error-code>404</error-code>
    	<location>/ErrorPage.jsp</location>
	</error-page>
	
	<session-config>
    	<session-timeout>10</session-timeout>
  	</session-config>
  
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
  		<servlet-name>HomePage</servlet-name>
    	<servlet-class>searchouse.servlet.HomePage</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>HomePage</servlet-name>
    	<url-pattern>/index</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>LogIn</servlet-name>
    	<servlet-class>searchouse.servlet.LogIn</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>LogIn</servlet-name>
    	<url-pattern>/login</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>LogOut</servlet-name>
    	<servlet-class>searchouse.servlet.LogOut</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>LogOut</servlet-name>
    	<url-pattern>/logout</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>SignUp</servlet-name>
    	<servlet-class>searchouse.servlet.SignUp</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>SignUp</servlet-name>
    	<url-pattern>/signUp</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>UpdateProfile</servlet-name>
    	<servlet-class>searchouse.servlet.UpdateProfile</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>UpdateProfile</servlet-name>
    	<url-pattern>/profile</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>InsertAd</servlet-name>
    	<servlet-class>searchouse.servlet.InsertAd</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>InsertAd</servlet-name>
    	<url-pattern>/newAd</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>MyAds</servlet-name>
    	<servlet-class>searchouse.servlet.MyAds</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>MyAds</servlet-name>
    	<url-pattern>/myAds</url-pattern>
  	</servlet-mapping>
 
  	 	
  	<servlet>
  		<servlet-name>searchAds</servlet-name>
    	<servlet-class>searchouse.servlet.SearchAds</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
    	<servlet-name>searchAds</servlet-name>
    	<url-pattern>/searchAds</url-pattern>
  	</servlet-mapping>
  	
  	
</web-app>
